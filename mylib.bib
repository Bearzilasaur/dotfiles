@article{hearn1997,
  title = {The {{Summer Buoyancy Dynamics}} of a {{Shallow Mediterranean Estuary}} and {{Some Effects}} of {{Changing Bathymetry}}: {{Tomales Bay}}, {{California}}},
  volume = {45},
  issn = {0272-7714},
  shorttitle = {The {{Summer Buoyancy Dynamics}} of a {{Shallow Mediterranean Estuary}} and {{Some Effects}} of {{Changing Bathymetry}}},
  abstract = {A numerical study is presented of the summer buoyancy dynamics of Tomales Bay (California, U.S.A.) which is an example of a shallow mediterranean estuary. Tomales Bay lies along the line of the San Andreas fault, and the resultant unidirectionality of the estuary greatly simplifies its hydrodynamics. The numerical model hindcasts the temperature and salinity of Tomales Bay from 1987 to 1993 using detailed hydrological and meteorological data, and the results are compared with field observations made during that 7-year period. A second simulation is made over the same 7-year period but using the bathymetry for 1861 when the estuary was longer and deeper. This allows an analysis to be made of the effects on the summer buoyancy dynamics of anthropogenic bathymetry changes. The model predicts that the historical estuary experienced greater negative buoyancy inputs during the late summer periods, and became more inverse with faster dispersion near the head of the basin.},
  number = {4},
  journal = {Estuarine, Coastal and Shelf Science},
  doi = {10.1006/ecss.1996.0197},
  author = {Hearn, C. J. and Largier, J. L.},
  month = oct,
  year = {1997},
  keywords = {bathymetry,buoyancy,California coast,estuaries,flushing time,historical,models,salinity variations},
  pages = {497-506},
  file = {/Users/tinabaum/Zotero/storage/U9NMFDVY/Hearn and Largier - 1997 - The Summer Buoyancy Dynamics of a Shallow Mediterr.pdf;/Users/tinabaum/Zotero/storage/D3QA95L7/S0272771496901973.html}
}


