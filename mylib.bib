@article{sternberg1971,
  title = {Measurements of Incipient Motion of Sediment Particles in the Marine Environment},
  volume = {10},
  issn = {0025-3227},
  abstract = {During the past few years a series of measurements has been made of flow conditions and associated sediment motion occurring in several high-velocity tidal channels in Puget Sound, Washington. Measurements consist of the velocity distribution occurring within 2 m of the bed, visual observation of the occurrence of incipient grain movement, and time-lapse photography of the seabed. From these data estimates have been made of the mean bottom velocity ({\=U}100), shear velocity (U{${_\ast}$}) and Shield's friction coefficient (\texttheta{}t) associated with the sediment movement. Results indicate that for mean grain sizes ranging from 0.30 mm to 1.10 mm and a variety of bed configurations the critical values of {\=U}100, U{${_\ast}$}, and \texttheta{}t required to initiate general sediment motion agree with experimentally derived competency curves. The bed-load transport of sediment corresponding to the occurrence of general sediment motion for grain sizes approximately 0.40 mm in diameter is estimated to be 8.4{$\cdot$}10-3 g cm-1sec-1. This movement is associated with mean velocity ({\=U}100) and shear velocity (U{${_\ast}$}) of 38.5 and 2.30 cm/sec, respectively.},
  number = {2},
  journal = {Marine Geology},
  doi = {10.1016/0025-3227(71)90049-1},
  author = {Sternberg, Richard W.},
  month = feb,
  year = {1971},
  pages = {113-119}
}

